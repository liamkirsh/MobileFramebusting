{
    "manifest_version": 2,

    "name": "Selenium Framebusting Info Collector",
    "description": "Collects framebusting info for Selenium",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "key": "",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "activeTab",
        "storage",
        "<all_urls>"
    ]
}
